{
    "version": 2,
    "builds": [
      {
        "src": "app.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["node_modules/.prisma/**", "node_modules/@prisma/client/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.ts"
      }
    ]
  }
  